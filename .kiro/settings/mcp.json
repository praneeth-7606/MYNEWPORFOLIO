{
  "mcpServers": {
    "github": {
      "command": "uvx",
      "args": ["mcp-server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      },
      "disabled": false,
      "autoApprove": [
        "list_repositories",
        "get_repository",
        "search_repositories",
        "get_file_contents"
      ]
    }
  }
}
